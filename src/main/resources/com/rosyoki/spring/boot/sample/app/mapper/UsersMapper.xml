<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rosyoki.spring.boot.sample.app.mapper.UsersMapper">
    <select id="select" resultType="com.rosyoki.spring.boot.sample.app.entity.Users">
        SELECT id, login_name, passwd FROM users WHERE id = #{id}
    </select>
</mapper>